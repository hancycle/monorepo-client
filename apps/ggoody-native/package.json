{
  "name": "ggoody-native",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "APP_ENV=dev expo start",
    "start:stg": "APP_ENV=stg expo start",
    "start:prod": "APP_ENV=prod expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "APP_ENV=dev expo start --android",
    "android:stg": "APP_ENV=stg expo start --android",
    "android:prod": "APP_ENV=prod expo start --android",
    "ios": "APP_ENV=dev expo start --ios",
    "ios:stg": "APP_ENV=stg expo start --ios",
    "ios:prod": "APP_ENV=prod expo start --ios",
    "web": "APP_ENV=dev expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:dev": "eas build --profile development",
    "build:stg": "eas build --profile preview",
    "build:prod": "eas build --profile production"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hancycle/constants": "*",
    "@hancycle/ui-react-native": "*",
    "expo": "^53.0.9",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "dotenv": "^16.4.5",
    "typescript": "^5.3.3"
  }
